{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["ES2020"],
   
    "module": "commonjs",
    "target": "ES2020",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "__tests__/*.(spec|test).ts"]
}
