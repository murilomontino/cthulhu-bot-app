{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "lib": [
      "esnext",
      "DOM",
      "DOM.Iterable",
      
    ],
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    
    "moduleResolution": "node"
  }
}
