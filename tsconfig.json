{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "jsx": "react-native",
    "target": "ES2020",
    "module":"CommonJS",
    "lib": [
      "es2020"
    ],
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
    
  },
  "include": ["src"]
  
}
